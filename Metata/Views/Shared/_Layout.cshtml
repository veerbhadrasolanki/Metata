<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Metata</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/convertor/ImageToBase64.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/Metata.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com" asp-append-version="true">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin  asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@300&display=swap" rel="stylesheet"  asp-append-version="true">
</head>
<body>
    <header class="bg-head">
        <nav class="navbar bg-head navbar-expand-sm   navbar-toggleable-sm navbar-light box-shadow">
            <div class="container-fluid bg-head">
                <a class="navbar-brand"  asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/img/Metata.png" width="30" height="30" class="d-inline-block align-top" alt="Metata">
                   Metata
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    


    <div class="container-fluid" style="padding:0px;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer  class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Metata - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script> 
    <script src="~/js/convertor/ImageToBase64.js" asp-append-version="true"></script>
    <script src="~/js/convertor/Base64ToImage.js" asp-append-version="true"></script>
    <script src="~/js/convertor/WeightConvertor.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
